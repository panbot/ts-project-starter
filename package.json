{
  "name": "ts-project-starter",
  "version": "0.0.1",
  "description": "a typescript project starter with modular framework",
  "scripts": {
    "start": "nodemon --delay 250ms -i dist -i node_modules -i src/fiddle --exec ./node_modules/.bin/ts-node -- ./src/run.ts src/my-app-name.ts",
    "fiddle": "ts-node src/run.ts",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:panbot/ts-project-starter.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bson": "^4.2.2",
    "fastify": "^3.11.0",
    "fastify-cors": "^4.1.0",
    "mongodb": "^3.6.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.8",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.7"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  }
}
