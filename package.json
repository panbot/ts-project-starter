{
  "name": "ts-project-starter",
  "version": "0.0.1",
  "description": "a typescript project starter with modular framework",
  "scripts": {
    "start": "nodemon --delay 250ms -i dist -i node_modules -i src/fiddle --exec ./node_modules/.bin/ts-node -- ./src/run.ts src/my-app-name.ts",
    "fiddle": "ts-node src/run.ts",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:panbot/ts-project-starter.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bson": "^4.2.2",
    "fastify": "^3.11.0",
    "fastify-cors": "^4.1.0",
    "ldap-authentication": "^2.2.6",
    "mongodb": "^3.6.3",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@types/node": "^14.14.8",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
